{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "compile",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/bundle.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}",
			"preLaunchTask": "webpack dev"
		},
		{
			"name": "Run",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/bundle.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}"
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}",
			"localRoot": "${workspaceRoot}",
			"remoteRoot": null
		}
	]
}