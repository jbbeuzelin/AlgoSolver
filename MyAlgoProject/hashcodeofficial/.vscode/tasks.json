{
	"version": "0.1.0",
	"command": "${workspaceRoot}/../../node_modules/.bin/webpack",
	"isShellCommand": true,
	"args": [
		"--display-modules",
		"--progress",
		"--display-error-details",
		"--config",
		"webpack.config.js"
	],
	"echoCommand": true,
	"tasks": [
		{
			"args": [],
			"suppressTaskName": true,
			"taskName": "webpack dev",
			"isBuildCommand": true
		},
		{
			"args": [
				"-p"
			],
			"suppressTaskName": true,
			"taskName": "webpack dist",
			"isBuildCommand": false
		}
	]
}